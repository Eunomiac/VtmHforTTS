<Defaults>


	<GridLayout class="houseSelectionGrid"
		width = "1260"
		height = "580"
		offsetXY="0 50"
		spacing = "20 20"
		cellSize = "300 180"
		childForceExpandWidth = "False"
		childForceExpandHeight = "False"
		constraintCount = "4" />

	<Text class="houseSelectionHeader"
		offsetXY = "0 -100" />

	<Image class="houseFlag"
		color="rgba(0.5, 0.5, 0.5, 1)"
		outlineSize="50" />

	<Image class="houseHover"
		color = "rgba(0, 0, 0, 0)"
		raycastTarget = "True"
    onMouseDown="HUD_Click"
    onMouseEnter="HUD_HoverOn"
		onMouseExit="HUD_HoverOff" />

	<Panel class="houseInfo"
		rectAlignment="LowerCenter"
		offsetXY="0 25"
		height="250"
		width="750" />

	<Image class="houseInfo" active = "False" />


</Defaults>


