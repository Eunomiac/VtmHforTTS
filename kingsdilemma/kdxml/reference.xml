<Defaults>
	<HorizontalLayout class="ref-hud-controls"
		rectAlignment = "LowerRight"
		childAlignment = "LowerRight"
		childForceExpandHeight = "false"
		childForceExpandWidth = "false"
		spacing = "0" />

	<Panel class="ref-hud-button-base"
		preferredHeight = "50" flexibleHeight = "0"
		preferredWidth = "50" flexibleWidth = "0" />

	<Image class="ref-hud-button" color="rgba(1, 1, 1, 0.2)" />
	<Image class="ref-hud-control" raycastTarget="true" onMouseEnter="Ref_HUD_HoverOn" onMouseExit = "Ref_HUD_HoverOff" />
	<Image class="ref-hud-button-bg" color="rgba(1, 1, 1, 0.01)" />
	<Image class="ref-hud-fade" color="rgba(1, 1, 1, 1)" active="false" />

	<Panel class="ref-hud-submenu"
		active = "false"
		width = "200" height = "150"
		onMouseExit = "Ref_HUD_HoverOff"
		position = "30 27"
		pivot = "1 0"  />

	<VerticalLayout class="ref-hud-submenu"
		preferredWidth = "200" flexibleWidth = "0" minWidth = "200"
		rectAlignment = "LowerRight"
		childAlignment="LowerRight"
		childForceExpandHeight="false"
		childForceExpandWidth="false"
		spacing="2" />

	<Panel class="submenu-option-container"
		preferredHeight = "30" flexibleHeight = "0"
		preferredWidth = "200" flexibleWidth = "0" minWidth = "200"
		onMouseEnter = "Ref_HUD_HoverOn"
		onMouseExit = "Ref_HUD_HoverOff"
		onMouseDown = "Ref_HUD_Click" />
	<Panel class="submenu-option-disabled"
		active = "false"
		onMouseEnter = ""
		onMouseExit = ""
		onMouseDown = ""
		color = "rgba(0, 0, 0, 0.2)" />


	<Image class="submenu-option-bg" image="ref-floating-bg" height="100%" width="100%" preserveAspect="false" />
	<Text class="submenu-option" font="Fonts/Friz"  alignment="MiddleCenter" fontSize="16" />
	<Image class="ref-hud-image" height="90%" active="false" />

</Defaults>

